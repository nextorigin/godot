  "repository": {
    "type": "git",
    "url": "https://github.com/nextorigin/godot2.git"
  },
  "dependencies": {
    "back": "~0.2.0",
    "clone": "~0.1.18",
    "graphite": "~0.0.6",
    "ip": "~0.3.2",
    "ndjson": "^1.4.3",
    "node-uuid": "~1.4.1",
    "readable-stream": "~1.0.33",
    "redis": "~0.12.1",
    "sendgrid-web": "~0.0.5",
    "telenode": "~0.0.3",
    "utile": "~0.2.1",
    "window-stream": "~0.7.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "collect-stream": "^1.1.1",
    "coveralls": "^2.11.12",
    "iced-coffee-coverage": "^1.0.3",
    "iced-coffee-script": "^108.0.11",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "one-time": "0.0.3",
    "optimist": "~0.6.0",
    "r...e": "~0.2.0",
    "vows": "~0.8.1"
  },
  "main": "./lib/godot",
  "engines": {
    "node": "0.10.x"
  },
  "scripts": {
    "test": "npm run test:modules && npm run test:coverage",
    "test:modules": "mocha --recursive --compilers coffee:iced-coffee-script/register --require iced-coffee-coverage/register test/*/*.coffee",
    "test:coverage": "istanbul report",
    "build": "iced --bare --map --runtime node -o lib/ -c src/",
    "watch": "iced --watch --bare --map --runtime node -o lib/ -c src/",
    "test-old": "vows --spec -i"
  }
}
